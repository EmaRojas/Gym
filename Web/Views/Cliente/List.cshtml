
@{
    ViewBag.Title = "List";
}
<script src="~/Content/js/Proyecto/Cliente.js"></script>
<script src="~/Content/js/Proyecto/FichaCliente.js"></script>
<div class="col s12 m12 l12" id="listClient">
    <div class="card-panel">
        <h4 class="header2 center-align">Clientes</h4>
        <div class="divider deep-purple lighten-2"></div>
        <div class="row" style="padding:30px">
            <div class="col s12 m12 l12">
                <table id="data-table-simple" class="responsive-table display" cellspacing="0">
                    <thead>
                        <tr>
                            <th>Nombre</th>
                            <th>Apellido</th>
                            <th>Nacimiento</th>
                            <th>D.N.I.</th>
                            <th>Teléfono</th>
                            <th>Opciones</th>
                        </tr>
                    </thead>
                    <tbody id="tblCliente">
                    </tbody>
                </table>
                <div class="right" style="padding-top:40px">
                    <a onclick="NuevoCliente()" class="btn waves-effect waves-light deep-purple darken-2" name="action">Nuevo</a>
                </div>

            </div>
        </div>

    </div>
</div>

<script>
    $(document).ready(function () {
        ListaCliente();
    });
</script>

