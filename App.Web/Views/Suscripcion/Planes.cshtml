@model App.Web.Models.SuscripcionModel

<center>
    <h2 style="color:gray">Bienvenido <strong style="color:black">@Model.NombreCompleto</strong>, le avisamos que su Suscripción esta vencida <strong style="color:red">(@Model.FechaVencimiento)</strong>, renueve la misma seleccionando alguno de nuestro planes. Muchas Gracias </h2>
</center>

<center style="padding-top: 8%">
    <h2>Planes</h2>
</center>
<hr style="background-color:#293B71; height:2px">

<div class="row m-b-20 pricing-table-1">
    <div class="col-xs-12 col-lg-4 text-center t1">
        <div class="header bg-primary" style="padding:15px 0px 10px 0px">
            <h2>
                <span>Mensual</span>
            </h2>
            <h3>
                <span>$3000,00</span>
            </h3>
        </div>
        <div class="inner">
            <div class="table-responsive">
                <table class="table table-hover table-striped">
                    <tbody>
                        <tr>
                            <td>
                                <i class="fa fa-check color-success"></i>
                                <span class="text">Soporte</span>
                            </td>
                        </tr>
                    </tbody>
                </table>
                <a href="@Model.Mensual" class="btn btn-raised btn-primary btn-lg">Comprar</a>
            </div>
        </div>
    </div>
    <div class="col-xs-12 col-lg-4 text-center t2">
        <div class="header bg-warning" style="padding:15px 0px 10px 0px">
            <h2>Semestral</h2>
            <h3>
                <span>$17000,00</span>
            </h3>
        </div>
        <div class="inner">
            <div class="table-responsive">
                <table class="table table-hover table-striped">
                    <tbody>
                        <tr>
                            <td>
                                <i class="fa fa-check color-success"></i>
                                <span class="text">Soporte</span>
                            </td>
                        </tr>
                    </tbody>
                </table>
                <a href="@Model.Semestral" class="btn btn-raised btn-warning btn-lg">Comprar</a>
            </div>
        </div>
    </div>
    <div class="col-xs-12 col-lg-4 text-center t3">
        <div class="header bg-info" style="padding:15px 0px 10px 0px">
            <h2>Anual</h2>
            <h3>
                <span>$33000,00</span>
            </h3>
        </div>
        <div class="inner">
            <div class="table-responsive">
                <table class="table table-hover table-striped">
                    <tbody>
                        <tr>
                            <td>
                                <i class="fa fa-check color-success"></i>
                                <span class="text">Soporte</span>
                            </td>
                        </tr>
                    </tbody>
                </table>
                <a href="@Model.Anual" class="btn btn-raised btn-info btn-lg">Comprar</a>
            </div>
        </div>
    </div>
</div>
